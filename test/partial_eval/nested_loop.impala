fn g(n: int, mut sum: int) -> int {
    if (n == 42) {
        for (let mut j = 0; j < 10; ++j) {
            ++sum;
        }
    }
    
    return sum;
}

fn f(n: int) -> int {
    let mut sum_xxx = 0;
    for (let mut i = 0; i < 10; ++i) {
        ++sum_xxx;
        sum_xxx = @g(n, sum_xxx);
    }

    return sum_xxx;
}

fn main(i: int) -> int {
    return f(i);
}
