fn take_address(&int) -> () {}

fn range(a: int, b: int, body: fn(int, fn())) -> () {
    if a < b {
        body(a);
        range(a+1, b, body, return)
    }
}

fn main() -> bool {
    let mut sum = 0;
    for i in range(0, 100000) {
        let mut foo = i;
        take_address(&foo);
        sum += foo
    }
    sum == 5000000000
}
