struct Vec3 {
    x: float,
    y: float,
    z: float,
}

fn set_one(mut v: Vec3) -> Vec3 {
    v.x = 1.f;
    v.y = 1.f;
    v.z = 1.f;
    v
}

fn cmp(cond: bool) -> bool {
    let mut v = Vec3{x: 0.f, y: 0.f, z: 0.f};
    if cond @{
        v = set_one(v);
    }
    v.x == 1.f
}

fn main() -> bool {
    cmp(true)
}
