fn range(a: int, z: int, body: fn(int)->()) -> () {
    if a < z {
        body(a);
        range(a+1, z, body, return)
    }
}

fn main(n : int) -> int {
    let mut res : int = 0;
    //for (let mut i : int = 0; i < n; ++i) {
    for i in range(0, n) {
        if i*i > 42 {
            break()
        }
        res += i;
    }
    res
}
