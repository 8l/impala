struct Foo {
    data: [i32 * 2],
    id: i32
}

fn foo() -> Foo {
    let mut stack: Foo;
    stack.id = 2323;
    stack.data(0) = 4242;
    stack
}

fn take_address(mut stack: &Foo) -> () {}

fn main() -> bool {
    let mut stack = foo();
    take_address(&stack);
    stack.data(0) + stack.id == 6565
}
